	    k12.in <-- { delay = 100ms; } <-- k21.out;
	  	
	  	k12.out --> { delay = 100ms; } --> k21.in;
	    k12.in <-- { delay = 100ms; } <-- k21.out;
	    
	   	k12.out --> { delay = 100ms; } --> k21.in;
	    k12.in <-- { delay = 100ms; } <-- k21.out;
	 
