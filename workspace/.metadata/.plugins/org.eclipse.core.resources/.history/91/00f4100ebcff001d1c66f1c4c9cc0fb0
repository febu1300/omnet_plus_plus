    	input in;
    	output out;
}
simple Knot2{
	gates: 
    	input in;
    	output out;
}
simple Knot3{
	gates: 
    	input in;
    	output out;
}

network Net1
{ 
	submodules:
    	k12: Knot1 {}
    	k21: Knot1 {}
    
   		k23: Knot2 {}
    	k32: Knot2 {}
    
   		k13: Knot3 {}
    	k31: Knot3 {}
    	
	connections:
